<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wujie.springbootlayui.bus.mapper.StaffCardMapper">


	<select id="getBinded" resultType="java.lang.String">
       select  GROUP_CONCAT(card_binder SEPARATOR ',') binder from bus_staff_card where belong_id=#{belongId}
    </select>

	<delete id="deleteStaffCardByUid">
        delete from bus_staff_card where belong_id = #{belongId}
    </delete>
</mapper>