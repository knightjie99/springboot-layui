<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.wujie.springbootlayui.bus.mapper.GateMapper">    <resultMap type="com.wujie.springbootlayui.bus.entity.Gate" id="gateMap" autoMapping="true">        <result property="id" column="id" jdbcType="INTEGER"/>    </resultMap>    <sql id="allSql">        id , device_sn ,gate_name, online_status, install_location,ver_type,         gate_status,current_ver,newest_ver,face_set,infrared_set,available,        temperature_set,belong_id,remark,active_time, create_time, update_time    </sql>    <select id="getIdByDeviceSn" resultType="java.lang.Integer">      select id from face_server.bus_gate where device_sn = #{deviceSn} and available=1   </select>    <select id="getGateById" resultMap="gateMap">        select        <include refid="allSql"></include>        from face_server.bus_gate        where id = #{id}    </select>    <select id="getAllGatesByVerType" resultMap="gateMap">        select        <include refid="allSql"></include>        from face_server.bus_gate        where ver_type = #{verType}    </select>    <select id="getAllOnlineGateByBelong" resultMap="gateMap">        select        <include refid="allSql"></include>        from face_server.bus_gate        where belong_id = #{belongId}        and online_status = 1    </select>    <delete id="deleteGateByUid">        delete from bus_gate where belong_id = #{belongId}    </delete></mapper>