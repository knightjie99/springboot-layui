<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.wujie.springbootlayui.sys.mapper.DeviceMapper">    <resultMap type="com.wujie.springbootlayui.sys.entity.Device" id="deviceMap" autoMapping="true">        <result property="id" column="id" jdbcType="INTEGER"/>    </resultMap>    <sql id="allSql">        id ,device_sn,belong_id,cpu_id,status,create_time,update_time    </sql>    <select id="queryDeviceByDeviceSn" resultMap="deviceMap">        select        <include refid="allSql"></include>        from sys_device        where device_sn = #{deviceSn}    </select>    <select id="queryBelongIdByDeviceSn" resultType="java.lang.Integer">        select belong_id belongId from sys_device where device_sn = #{deviceSn}    </select>    <select id="selectSnGroup" resultType="String">        select GROUP_CONCAT(device_sn SEPARATOR ',') sn from sys_device    </select>    <update id="resetBelongId" >        update sys_device set belong_id = null where belong_id = #{belongId}    </update></mapper>