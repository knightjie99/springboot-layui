<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.wujie.springbootlayui.sys.mapper.AppMapper">    <select id="getPreviousId" resultType="java.lang.Integer">        <![CDATA[        select id from sys_app where id = (select max(id) from sys_app where id < #{id})           ]]>    </select>    <select id="getAppById" resultType="com.wujie.springbootlayui.sys.entity.App">        select * from sys_app where id = #{id}    </select></mapper>