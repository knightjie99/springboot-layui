<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.wujie.springbootlayui.sys.mapper.DeptMapper">    <resultMap type="com.wujie.springbootlayui.sys.entity.Dept" id="deptMap" autoMapping="true">        <result property="id" column="id" jdbcType="INTEGER"/>    </resultMap>    <sql id="allSql">        id , pid, name, open, remark, address, available, ordernum, createtime, belong_user    </sql>    <select id="getDeptById" resultMap="deptMap">        select        <include refid="allSql"></include>        from face_server.sys_dept        where id = #{id} and available =1    </select>    <delete id="deleteDeptByUid">        delete from sys_dept where belong_user = #{belongId}    </delete></mapper>